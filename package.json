{
    "name": "react-openapi-designer",
    "version": "0.1.1",
    "main": "dist/main.js",
    "scripts": {
        "typecheck": "flow check",
        "jest": "jest",
        "jest:watch": "jest --watch",
        "test": "npm run typecheck && npm run jest",
        "build": "NODE_ENV=production webpack --config webpack/webpack.prod.js --progress",
        "watch": "webpack --config webpack/webpack.common.js --watch  --progress",
        "dev": "webpack serve -c webpack/webpack.dev.js --hot --open"
    },
    "dependencies": {
        "@blueprintjs/core": "^3.40.0",
        "@blueprintjs/popover2": "^0.3.1",
        "@reduxjs/toolkit": "^1.5.0",
        "class-autobind-decorator": "^3.0.1",
        "json-schema-faker": "^0.5.0-rcv.33",
        "lodash": "^4.17.20",
        "prismjs": "^1.23.0",
        "prop-types": "^15.7.2",
        "react-redux": "^7.0.1",
        "react-simple-code-editor": "^0.11.0",
        "redux": "^4.0.1",
        "redux-thunk": "^2.3.0",
        "tailwindcss": "^2.0.3",
        "type-of-is": "^3.5.1"
    },
    "peerDependencies": {
        "react": ">=16.13"
    },
    "jest": {
        "setupFilesAfterEnv": [
            "./__jest__/setup-after-env.js"
        ],
        "testMatch": [
            "**/__tests__/**/*.test.js?(x)"
        ],
        "verbose": false,
        "resetMocks": true,
        "resetModules": true,
        "coveragePathIgnorePatterns": [
            "/node_modules/"
        ]
    },
    "devDependencies": {
        "@babel/core": "^7.9.0",
        "@babel/plugin-proposal-class-properties": "^7.12.13",
        "@babel/plugin-proposal-decorators": "^7.8.3",
        "@babel/plugin-proposal-optional-chaining": "^7.9.0",
        "@babel/plugin-transform-runtime": "^7.12.10",
        "@babel/preset-env": "^7.12.17",
        "@babel/preset-flow": "^7.12.13",
        "@babel/preset-react": "^7.12.13",
        "autoprefixer": "^10.2.5",
        "babel-loader": "^8.0.6",
        "babel-plugin-lodash": "^3.3.4",
        "babel-plugin-prismjs": "^2.0.1",
        "buffer": "^6.0.3",
        "css-loader": "^5.1.1",
        "file-loader": "^6.2.0",
        "html-webpack-plugin": "^5.2.0",
        "https-browserify": "^1.0.0",
        "jest": "^26.6.3",
        "postcss": "^8.2.7",
        "postcss-loader": "^5.1.0",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "stream-http": "^3.1.1",
        "style-loader": "^2.0.0",
        "terser-webpack-plugin": "^5.1.1",
        "vm-browserify": "^1.1.2",
        "webpack": "^5.23.0",
        "webpack-bundle-analyzer": "^4.4.0",
        "webpack-cli": "^4.5.0",
        "webpack-dev-server": "^3.11.2",
        "webpack-merge": "^5.7.3"
    }
}
